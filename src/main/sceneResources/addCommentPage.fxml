<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="750.0" prefWidth="1200.0" styleClass="pane" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.client.graphicView.scenes.AddCommentPage">
    <stylesheets>
        <URL value="@Main/client/graphicView/cssFiles/salesHistory.css"/>
        <URL value="@Main/client/graphicView/cssFiles/basicStyles.css"/>
        <URL value="@Main/client/graphicView/cssFiles/listView.css"/>
    </stylesheets>
    <children>
        <Label alignment="CENTER" layoutX="333.0" layoutY="121.0" prefHeight="62.0" prefWidth="117.0" text="title">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="333.0" layoutY="228.0" prefHeight="62.0" prefWidth="117.0" text="content">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <TextField fx:id="title" layoutX="450.0" layoutY="121.0" prefHeight="62.0" prefWidth="420.0">
            <font>
                <Font size="30.0"/>
            </font>
        </TextField>
        <TextField fx:id="content" alignment="TOP_LEFT" layoutX="450.0" layoutY="228.0" prefHeight="386.0"
                   prefWidth="420.0">
            <font>
                <Font size="30.0"/>
            </font>
        </TextField>
        <Button layoutX="551.0" layoutY="652.0" mnemonicParsing="false" onAction="#submitComment" prefHeight="62.0"
                prefWidth="98.0" text="submit">
            <font>
                <Font size="30.0"/>
            </font>
        </Button>
        <ImageView fitHeight="63.0" fitWidth="63.0" layoutX="1101.0" layoutY="651.0" onMouseClicked="#goBack"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Main/client/graphicView/resources/images/back.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="104.0" fitWidth="80.0" layoutX="26.0" layoutY="651.0" onMouseClicked="#logout" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Main/client/graphicView/resources/images/logout.png" />
            </image>
        </ImageView>
    </children>

</AnchorPane>
