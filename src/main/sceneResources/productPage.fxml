<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.graphicView.scenes.ProductPage">
   <stylesheets>
      <URL value="@../java/Main/graphicView/cssFiles/basicStyles.css" />
   </stylesheets>
   <children>
      <ImageView fitHeight="63.0" fitWidth="63.0" layoutX="1101.0" layoutY="651.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../java/Main/graphicView/resources/images/back.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="63.0" fitWidth="63.0" layoutX="1021.0" layoutY="651.0" onMouseClicked="#goToUserPanelPage" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../java/Main/graphicView/resources/images/avatar.png" />
         </image>
      </ImageView>
      <ImageView fx:id="scoreImage" fitHeight="50.0" fitWidth="200.0" layoutX="151.0" layoutY="311.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="scoreLabel" alignment="CENTER" layoutX="50.0" layoutY="311.0" prefHeight="50.0" prefWidth="100.0" text="Label">
         <font>
            <Font size="30.0" />
         </font></Label>
      <ScrollPane fx:id="scrollImage" layoutX="50.0" layoutY="50.0" prefHeight="244.0" prefWidth="301.0" />
      <ScrollPane layoutX="460.0" layoutY="50.0" prefHeight="140.0" prefWidth="704.0">
         <content>
            <VBox prefHeight="137.0" prefWidth="703.0">
               <children>
                  <TextArea fx:id="generalFeatures" editable="false" prefHeight="137.0" prefWidth="703.0">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </TextArea>
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <ScrollPane fx:id="specialFeaturesPane" layoutX="460.0" layoutY="205.0" prefHeight="140.0" prefWidth="704.0" visible="false">
         <content>
            <VBox prefHeight="132.0" prefWidth="703.0">
               <children>
                  <TextArea fx:id="specialFeatures" editable="false" prefHeight="200.0" prefWidth="703.0">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </TextArea>
               </children>
            </VBox>
         </content></ScrollPane>
      <ImageView fitHeight="73.0" fitWidth="73.0" layoutX="934.0" layoutY="646.0" onMouseClicked="#goToSelectSellerPage" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../java/Main/graphicView/resources/images/addToCart.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
