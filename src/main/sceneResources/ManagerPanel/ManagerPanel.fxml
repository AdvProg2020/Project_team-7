<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="parentAnchor" fx:id="parentAnchor" prefHeight="750.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Main.graphicView.scenes.ManagerPanel.ManagerPanelController">
    <stylesheets>
        <URL value="@../../java/Main/graphicView/cssFiles/basicStyles.css"/>
        <URL value="@../../java/Main/graphicView/cssFiles/buyerManagerPanel.css"/>
    </stylesheets>
    <children>
        <Button layoutX="128.0" layoutY="188.0" mnemonicParsing="false" onAction="#goToPersonalInformation"
                prefHeight="104.0" prefWidth="420.0" text="Personal Information"/>
        <Button layoutX="128.0" layoutY="331.0" mnemonicParsing="false" onAction="#goToManageUsers" prefHeight="104.0"
                prefWidth="420.0" text="Manage Users"/>
        <Button layoutX="128.0" layoutY="473.0" mnemonicParsing="false" onAction="#goToManageRequests"
                prefHeight="104.0" prefWidth="420.0" text="Manage Requests"/>
        <Button layoutX="612.0" layoutY="188.0" mnemonicParsing="false" onAction="#goToManageCategories"
                prefHeight="104.0" prefWidth="420.0" text="Manage Categories"/>
        <Button layoutX="612.0" layoutY="331.0" mnemonicParsing="false" onAction="#goToManageDiscounts"
                prefHeight="104.0" prefWidth="420.0" text="Manage Discounts"/>
        <Button layoutX="612.0" layoutY="473.0" mnemonicParsing="false" onAction="#goToManageProducts"
                prefHeight="104.0" prefWidth="420.0" text="Manage Products"/>
        <ImageView fitHeight="104.0" fitWidth="80.0" layoutX="20.0" layoutY="20.0" onMouseClicked="#goBack"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../java/Main/graphicView/resources/images/back.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="104.0" fitWidth="80.0" layoutX="1081.0" layoutY="650.0" onMouseClicked="#logout"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../java/Main/graphicView/resources/images/logout.png"/>
            </image>
        </ImageView>
    </children>
</AnchorPane>
